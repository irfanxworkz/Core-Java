package com.xworkz.Object.boot;

import com.xworkz.Object.thing.Paint;

public class AllRunner {

	public static void main(String[] args) 
	{
		Paint paint=new Paint();
		paint.setBrand("Asian Paint");
		paint.setPrice(5500);
		paint.setColor("Blue");
		paint.setQuantity(15);
		System.out.println(paint);
		System.out.println("Hash code of paint is: "+paint.hashCode()+" Original hashCode of paint is :"+System.identityHashCode(paint));
		
		Paint paint1=new Paint();
		paint.setBrand("Asian Paint");
		paint.setPrice(5500);
		paint.setColor("Blue");
		paint.setQuantity(15);
		
		System.out.println(paint1);
		System.out.println(paint.equals(paint1));
		
		System.out.println("--------------------------------------------------------------------------------------------------------------");
		
		

	}

}
