package com.xworkz.Inheritance18.thing;

public class Converter extends Sport
{
	public String display(String sport)
	{
		System.out.println("Sport:"+sport);
		this.display(sport);
		T20Cricket t20 = new T20Cricket();
		t20.getSponsorName();
		t20.getCoachName();
		t20.getCountry();
		
		return sport;
	}

}
