package com.xworkz.Concept.boot;

import com.xworkz.Concept.thing.Cake;

public class CakRrunner {

	public static void main(String[] args) {
		
		Cake cack1=new Cake("Chocklete",400,"round,",350);
		System.out.println(cack1);
		
		Cake cack2=new Cake("Chocklete",400,"round,",350);
		System.out.println(cack2);
		
		Cake cack3=new Cake("Chocklete",300,"Heart,",550);
		System.out.println(cack3);
		
		boolean equals=cack1.equals(cack2);
		System.out.println("both are same :"+equals);
		System.out.println("-----------");
		
		boolean equals1=cack1.equals(cack3);
		System.out.println("both are Same:"+equals1);
		System.out.println("-----------");

	}

}
