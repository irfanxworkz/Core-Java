package com.xworkz.Object.boot;

import com.xworkz.Object.thing.Paint;

public class AllRunner {

	public static void main(String[] args) 
	{
		Paint paint=new Paint();
		paint.getBrand("Asian Paint");
		paint.getPrice(5500);
		paint.getColor("Blue");
		paint.getQuantity(15);
		
		System.out.println(paint);
		
		System.out.println("Hash code of paint is: "+paint.hashCode()+"Original hashCode of paint is :");
		
		Paint paint1=new Paint();
		paint1.getBrand("Asian Paint");
		paint1.getPrice(5500);
		paint1.getColor("Blue");
		paint1.getQuantity(15);
		
		System.out.println(paint1);
		
		System.out.println(paint.equals(pant1));
		
		

	}

}
