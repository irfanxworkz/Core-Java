package com.xworkz.Concept.thing;

public class Tv 
{
	private String brand;
	private double price;
	private int size;
	
	public Tv(String brand,double price,int size)	
	{
		this.brand=brand;
		this.price=price;
		this.size=size;
	}
	
	@Override
	public boolean equals(Object obj) {
		
		System.out.println(""+obj);
		/*if(obj !=null)
		{
			System.out.println("tv1 is not null");
		}
		else
		{
			System.out.println("tv1 is null");
		}*/
		return super.equals(obj);
	}
	
	@Override
	public String toString() {
		System.out.println("Running toString in Tv class...");
		return "[Brand :"+this.brand+"] [Price :"+price+"] [Size :"+size+"] ";
	}
}
