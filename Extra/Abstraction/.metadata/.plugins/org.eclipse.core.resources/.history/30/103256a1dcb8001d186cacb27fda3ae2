package com.xworkz.abstraction.things;

public abstract class ATM {
	
		public int amount,digit,withdraw;
		public double fund,remaining;
		//public long number,aadhar,mobNumber;
		//public String tpye,name;
		public abstract void deposit(int amount);
		
		public abstract void withdrawals(int withdraw);
		
		public abstract double remainingAmount(double remaining);
		
		public abstract void transfer_Fund(double fund);
		
		public abstract void atm_Pin(int digit);
		
		public abstract void acc_Number(long number);
		
		public abstract void linked_Aadhar(long aadhar);
		
		public abstract double acc_Balance();
		
		public abstract void acc_Type(String type);
		
		public abstract void cust_Name(String name);
		
		public abstract void cust_Cont_Number(long mobNumber);
		
		
		
		
		

}
